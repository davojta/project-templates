# Environment Variables for Python Geo CLI
# Copy this file to .env and fill in your values

# Mapbox Access Token for better basemap rendering in KeplerGL
# Get your free token at: https://mapbox.com/help/create-api-access-token/
MAPBOX_ACCESS_TOKEN=your_mapbox_access_token_here

# Optional: Cache directory for OSM data downloads
# GEO_CLI_CACHE_DIR=./data/cache

# Optional: Log level (DEBUG, INFO, WARNING, ERROR)
# GEO_CLI_LOG_LEVEL=INFO

# Optional: Memory limit for SedonaDB in GB
# GEO_CLI_MAX_MEMORY_GB=8